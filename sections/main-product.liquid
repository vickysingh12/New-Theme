<script>

  function updateInventoryDisplay(product, block) {


    const inventoryManagement = product.inventory_management;
    const inventoryQuantity = product.inventory_quantity;
    const inventoryPolicy = product.inventory_policy;
    const inventoryThreshold = window.inventory_threshold;
    const showInventoryQuantity = window.show_inventory_quantity;
  
    let inventoryDisplay = '';
  
    if (inventoryManagement === 'shopify') {
      if (inventoryQuantity > 0) {
        if (inventoryQuantity <= inventoryThreshold) {
          inventoryDisplay = `
            <svg width="15" height="15" aria-hidden="true">
              <circle cx="7.5" cy="7.5" r="7.5" fill="rgba(238,148,65,0.3)"/>
              <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255,255,255)" stroke-width="1" fill="rgb(238,148,65)"/>
            </svg>
          `;
          if (showInventoryQuantity) {
            inventoryDisplay += `<span>${inventoryQuantity} in stock </span>`;
          } else {
            inventoryDisplay += `<span>Low stock</span>`;
          }
        } else {
          inventoryDisplay = `
            <svg width="15" height="15" aria-hidden="true">
              <circle cx="7.5" cy="7.5" r="7.5" fill="rgba(62,214,96,0.3)"/>
              <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255,255,255)" stroke-width="1" fill="rgb(62,214,96)"/>
            </svg>
          `;
          if (showInventoryQuantity) {
            inventoryDisplay += `<span>${inventoryQuantity} in stock</span>`;
          } else {
            inventoryDisplay += `<span>Low stock</span>`;
          }
        }
      } else {
        if (inventoryPolicy === 'continue') {
          inventoryDisplay = `
            <svg width="15" height="15" aria-hidden="true">
              <circle cx="7.5" cy="7.5" r="7.5" fill="rgba(62,214,96,0.3)"/>
              <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255,255,255)" stroke-width="1" fill="rgb(62,214,96)"/>
            </svg>
            <span>In stock</span>
          `;
        } else {
          inventoryDisplay = `
            <svg width="15" height="15" aria-hidden="true">
              <circle cx="7.5" cy="7.5" r="7.5" fill="rgba(200,200,200,0.3)"/>
              <circle cx="7.5" cy="7.5" r="5" stroke="rgb(255,255,255)" stroke-width="1" fill="rgb(200,200,200)"/>
            </svg>
            <span>Out of stock</span>
          `;
        }
      }
    }
  
    const inventoryElement = document.querySelector('.featured-product__inventory');
    inventoryElement.innerHTML = inventoryDisplay;
  }  

  let minusbtn = document.getElementById("quantity__button-minus")
  let addbtn = document.getElementById("quantity__button-plus")
  let featuredImg = document.querySelector(".featured-product__media-img-wrapper > img")
  let productInventory = document.querySelector(".featured-product__inventory  span")
  let featuredImgLower = document.querySelectorAll(".featured-product__media-lower img")
  let price = document.querySelector(".featured-product__price span")
  let mainProduct = {{ product.options | json }} ;
  let variantLength = [] ;
  
  let variants = {{ product.variants | json }} ;
  let allInputs = document.querySelectorAll(".featured-product__content input:not([type=number])")

  {% for item in product.variants %}
    variantLength.push( {{ item.inventory_quantity | json }} );
  {% endfor %}

  for( let i = 0 ; i < variants.length ; i++ ) {
    variants[ i ].inventory_quantity = variantLength[ i ];
  }
  variantLength = []

  {% for item in product.variants %}
    variantLength.push( {{ item.inventory_management | json }} );
  {% endfor %}

  for( let i = 0 ; i < variants.length ; i++ ) {
    variants[ i ].inventory_management = variantLength[ i ];
  }

  variantLength = [];
  {% for item in product.variants %}
    variantLength.push( {{ item.inventory_policy | json }} );
  {% endfor %}

  for( let i = 0 ; i < variants.length ; i++ ) {
    variants[ i ].inventory_policy = variantLength[ i ];
  }

  allInputs.forEach((inp) => {

    inp.addEventListener('change' , function () {

      let activeVariant = findActiveVariantId();
      
      if(activeVariant?.activeVariantId) {

        const url = new URL(location);
        url.searchParams.set("variant", activeVariant?.activeVariantId);
        history.pushState({}, "", url);
        //

        price.innerHTML = Shopify.formatMoney( activeVariant.activeVariantData.price )
        updateInventoryDisplay(activeVariant.activeVariantData);
        // productInventory.innerHTML =  ;
      }

      disableUnavailableVariants();

    })

  })

  featuredProductClosebtn.addEventListener('click' , function () {
    featuredProductModal.classList.toggle('hidden');
  })

  featuredImg.addEventListener('click' , function (e) {
    featuredProductModal.classList.toggle('hidden')
    featuredProductModal.scrollTo(0 , 0);
    let images = featuredProductModal.getElementsByTagName('img');

    for (var i = 0; i < images.length; i++) {
      if ( images[i].src.includes(e.target.src ) ) {
        featuredProductModal.insertBefore(images[i], images[0]);
        break;

      }
    }
  })
  
  featuredImgLower.forEach(( item ) => {
  
    item.addEventListener('click' , function (e) {

      featuredImgLower.forEach(( item ) => {
        item.classList.remove('black-border')
      })

      featuredImg.src = e.target.src ; 
      e.target.classList.add('black-border')

    })

  })

  addbtn.addEventListener('click' , function () {

    minusbtn.classList.remove('disabled');
    QuantityInput.value = +QuantityInput.value + 1;

    const event = new Event('change');
    QuantityInput.dispatchEvent(event);
  })

  minusbtn.addEventListener('click' , function (e) {

    if( QuantityInput.value == 1 ) {
      return;
    }

    QuantityInput.value = +QuantityInput.value - 1;

    if( QuantityInput.value == 1 ) {
      e.target.classList.add('disabled')
    }

     const event = new Event('change');
     QuantityInput.dispatchEvent(event);
  })

  function findActiveVariantId() {

    let variantId;
    let QuantityInput = document.getElementById("QuantityInput").value;
    let options = [];

    mainProduct.forEach(( item ) => {
      let selectedVal = document.querySelector(`[name="${item}"]:checked`)?.value;
      options.push(selectedVal);
    });

    variants.forEach(( item ) => {
      if(arraysMatch(options, item.options)) {
        variantId = item.id
      }
    });

    let activeVariantData = variants.find((variant)=> {
      return variant.id == variantId
    });

    
    if(activeVariantData?.featured_image?.src) {
      featuredImg.src = activeVariantData.featured_image?.src
    } else {
      //featuredImg.src = {{ product.featured_image |  json  }}
    }

    return { activeVariantId: variantId , quantity: QuantityInput , activeVariantData  }
  }

  productForm.addEventListener('submit' , function (e) {

    e.preventDefault();

    addToCart.parentElement.setAttribute("disabled", true);
    addToCart.classList.toggle('hidden');
    addToCartspinner.classList.toggle('hidden');

    let variantSelected = findActiveVariantId()

    let formData = {
      'items': [{
       'id': variantSelected.activeVariantId || variants?.[0]?.id ,
       'quantity': variantSelected.quantity
       }]
     }
     
    fetch('/cart/add.js', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify(formData)
    })
    .then(response => {
      return response.json();
    })
    .then(response => {
      addToCart.classList.toggle('hidden');
      addToCartspinner.classList.toggle('hidden');
      addToCart.parentElement.removeAttribute("disabled");

      if(response.status == 422) {
        throw response.description;
      }
      document.getElementsByTagName('cart-drawer')[0].open();
      document.getElementsByTagName('cart-drawer-items')[0].onCartUpdate();
     
    })
    .catch((error) => {
      showErrorPopup(error)
      console.error('Error:', error);
    })

  })

  {% if section.settings.image_zoom == 'hover' %}
    document.addEventListener("DOMContentLoaded", function() {
      const container = document.querySelector('.featured-product__media-img-wrapper');
      const image = container.querySelector('.featured-product__media-img-wrapper > img');
  
      container.addEventListener('mousemove', function(e) {
        const containerRect = container.getBoundingClientRect();
        const x = e.clientX - containerRect.left;
        const y = e.clientY - containerRect.top;
  
        const xPercent = (x / containerRect.width) * 100;
        const yPercent = (y / containerRect.height) * 100;
  
        image.style.transformOrigin = `${xPercent}% ${yPercent}%`;
        image.style.transform = 'scale(1.1)';
      });
  
      container.addEventListener('mouseleave', function() {
          image.style.transform = 'scale(1)';
      });
    });
  {% endif %}

  function disableUnavailableVariants() {
    const fieldsets = document.querySelectorAll('variant-selects fieldset');
    const selectedOptions = {};
    
    fieldsets.forEach(fieldset => {
      const name = fieldset.querySelector('input').name;
      const checkedInput = fieldset.querySelector('input:checked');
      selectedOptions[name] = checkedInput ? checkedInput.value : null;
      
      fieldset.querySelectorAll('input').forEach(input => input.disabled = false);
    })
    
    fieldsets.forEach(fieldset => {
        const name = fieldset.querySelector('input').name;

        fieldset.querySelectorAll('input').forEach(input => {

            const tempOptions = { ...selectedOptions, [name]: input.value };

            const matchingVariant = variants.find(variant => {
                return Object.keys(tempOptions).every(key => variant.options.includes(tempOptions[key]));
            });

            if ( !matchingVariant || matchingVariant.inventory_quantity === 0 ) {
                input.disabled = true;
            }
        });
    });
    
  }

  function selectFirstAvailableVariant() {
    const fieldsets = document.querySelectorAll('variant-selects fieldset');
    const selectedOptions = {};

    const trySelectCombination = (index) => {

        if (index >= fieldsets.length) {
            disableUnavailableVariants();
            return true;
        }

        const fieldset = fieldsets[index];
        const inputs = fieldset.querySelectorAll('input');

        for (let input of inputs) {

            if (!input.disabled) {
                selectedOptions[fieldset.querySelector('input').name] = input.value;

                if (trySelectCombination(index + 1)) {
                    return true;
                }
            }
        }

        if (fieldset.querySelector('input:checked')) {
            fieldset.querySelector('input:checked').checked = false;
        }

        return false;
    };

    trySelectCombination(0);
  }

  selectFirstAvailableVariant();

</script>

